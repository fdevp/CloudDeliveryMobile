<?xml version="1.0" encoding="utf-8" ?>
<resources>
  <style name="Salepoint"></style>
  <style name="Salepoint.Autocomplete">
    <item name="colorControlNormal">@color/primaryColor</item>
    <item name="colorControlActivated">@color/primaryColor</item>
  </style>
  <style name="Salepoint.Order"></style>
  <style name="Salepoint.Order.Details"></style>
  <style name="Salepoint.Order.Details.Field">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
  </style>
  <style name="Salepoint.Order.Details.Field.Bold">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:textStyle">bold</item>
  </style>
</resources>